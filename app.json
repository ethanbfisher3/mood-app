{
  "expo": {
    "name": "mood-app",
    "slug": "mood-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mood_tracker_icon1024.png",
    "scheme": "moodapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ethanbfisher3.moodapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/mood_tracker_icon1024.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ethanbfisher3.moodapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/mood_tracker_icon1024.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/mood_tracker_icon1024.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0da46bed-a9d2-4b81-82c5-040cf46898b5"
      }
    }
  }
}
